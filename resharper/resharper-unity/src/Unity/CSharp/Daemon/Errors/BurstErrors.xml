      <Tag externalName="BurstDebugLogInvalidArgumentWarning.HIGHLIGHTING_ID" default="WARNING">
        <LocalizedTitle resourceName="DebugLoggingFunctionsAcceptsOnlyStrings" resourceType="Strings" />
