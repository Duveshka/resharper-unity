      <Tag externalName="BurstForeachNotSupportedWarning.HIGHLIGHTING_ID" default="WARNING">
        <LocalizedTitle resourceName="TheforeachConstructionIsNotSupported" resourceType="Strings" />
        <LocalizedDescription resourceName="TheforeachConstructionIsNotSupported" resourceType="Strings" />
      </Tag>
      <Tag externalName="BurstLoadingStaticNotReadonlyWarning.HIGHLIGHTING_ID" default="WARNING">
        <LocalizedTitle resourceName="LoadingFromANonReadonlyStaticFieldIsNotSupported" resourceType="Strings" />
        <LocalizedDescription resourceName="LoadingFromANonReadonlyStaticFieldIsNotSupported" resourceType="Strings" />
      </Tag>
      <Tag externalName="BurstLoadingManagedTypeWarning.HIGHLIGHTING_ID" default="WARNING">
        <LocalizedTitle resourceName="LoadingManagedTypeIsNotSupported" resourceType="Strings" />
        <LocalizedDescription resourceName="LoadingManagedTypeIsNotSupported" resourceType="Strings" />
