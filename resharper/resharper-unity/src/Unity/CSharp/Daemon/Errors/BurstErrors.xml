    Strings = JetBrains.ReSharper.Plugins.Unity.Resources.Strings;
  </Usings>
  
  <SeverityConfiguration>
    <Group name="UnityHighlightingGroupIds.Burst">
      <Tag externalName="BurstAccessingManagedMethodWarning.HIGHLIGHTING_ID" default="WARNING">
        <LocalizedTitle resourceName="AccessingManagedMethodsIsNotSupported" resourceType="Strings" />
        <LocalizedDescription resourceName="AccessingManagedMethodsIsNotSupported" resourceType="Strings" />
      </Tag>
      <Tag externalName="BurstTryNotSupportedWarning.HIGHLIGHTING_ID" default="WARNING">
        <LocalizedTitle resourceName="TryStatementIsNotSupported" resourceType="Strings" />
        <LocalizedDescription resourceName="TryStatementIsNotSupported" resourceType="Strings" />
      </Tag>
      <Tag externalName="BurstFunctionSignatureContainsManagedTypesWarning.HIGHLIGHTING_ID" default="WARNING">
        <LocalizedTitle resourceName="FunctionSignatureCannotContainManagedTypes" resourceType="Strings" />
